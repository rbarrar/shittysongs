<header>
    <h1 class="text-center">Worst. Songs. Ever.</h1>
      <p class="text-center">(We apologize for the earworms in advance...)</p>
  </header>


<div class="row">
  <div class="col-md-6">
    <div class="video">
        <% @videos.each do |v| %>
        <h2><%= v.artist_name %> - <%= v.song_name %></h2>
        <iframe width="640" height="360" src= "<%= v.url %>" frameborder="0" allowfullscreen></iframe>
        <p> <%= v.description %> </p>
        <p> <%= commontator_thread(v) %></p>
        <% end %>
    </div>
  </div>
</div>
